{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(css|js))",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*\\.(html|htm))",
      "headers": {
        "Cache-Control": "public, max-age=300"
      }
    },
    {
      "src": "/image/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/favicon\\.ico",
      "dest": "/image/my-small1.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
