{
  "version": 2,
  "functions": {
    "server.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/*.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "public": true
}
