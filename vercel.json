{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(js|css|html|jpg|jpeg|png|gif|ico|svg|txt|xml|woff|woff2|ttf|eot))",
      "headers": {
        "Cache-Control": "public, max-age=3600, s-maxage=3600"
      },
      "dest": "/public/$1"
    },
    {
      "src": "/favicon\\.ico",
      "dest": "/public/images/my-small1.jpg"
    },
    {
      "src": "/favicon\\.png", 
      "dest": "/public/images/my-small1.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
